{
  "name": "vidoose-backend",
  "version": "1.0.0",
  "description": "Production grade video downloader backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:worker": "node dist/worker.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@cloudinary/transformation-builder-sdk": "^1.21.2",
    "@cloudinary/url-gen": "^1.22.0",
    "@fastify/autoload": "^6.3.1",
    "@fastify/compress": "^8.3.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.65.1",
    "cloudinary": "^2.0.0",
    "cookies-next": "^6.1.1",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "fastify-type-provider-zod": "^6.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "resend": "^6.5.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "prettier": "^3.7.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "fastify-type-provider-zod": {
      "fastify": "$fastify"
    }
  }
}
